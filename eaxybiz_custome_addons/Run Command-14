    @api.constrains('mobile')
    def get_check_mobile(self):
        for rec in self:
            if rec.mobile and len(rec.mobile) != 10:
                raise ValidationError("Your given mobile number is incorrect!!!")
                
                
                
                
                
                
    @api.constrains('aadhar')
    def get_check_aadhar(self):
        for rec in self:
            if rec.aadhar and len(rec.aadhar) != 12:
                raise ValidationError("Your given aadhar number is incorrect!!!")
                
                
                
